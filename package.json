{
  "name": "@nb/nestjs-accountsjs",
  "version": "1.2.0",
  "description": "Use Accounts JS in nest like a pro",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "index.js",
    "index.ts",
    "index.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:integration": "jest --config ./tests/jest-e2e.json"
  },
  "author": "Nick Bolles <me@nickbolles.com>",
  "license": "ISC",
  "devDependencies": {
    "@nestjs/common": "6.3.1",
    "@nestjs/core": "6.3.1",
    "@nestjs/platform-express": "6.3.1",
    "@nestjs/testing": "6.3.1",
    "@types/jest": "24.0.13",
    "@types/node": "12.0.4",
    "husky": "1.3.1",
    "jest": "24.8.0",
    "lint-staged": "8.1.7",
    "prettier": "1.17.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^2.6.3",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.2.0",
    "typescript": "3.5.1"
  },
  "peerDependencies": {
    "@nestjs/core": "^6.2.0",
    "reflect-metadata": "^0.1.12"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --ignore-path ./.prettierignore --write",
      "git add"
    ]
  },
  "dependencies": {
    "@accounts/graphql-api": "^0.15.0",
    "@accounts/rest-express": "^0.15.0",
    "@accounts/server": "^0.15.0",
    "@accounts/types": "^0.15.0",
    "@graphql-modules/core": "^0.7.6",
    "@nestjs/graphql": "^6.2.4"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  }
}
