{
  "name": "@nb/nestjs-accountsjs",
  "version": "1.12.0",
  "description": "Use Accounts JS in nest like a pro",
  "main": "dist/index.js",
  "files": [
    "dist/*",
    "index.js",
    "index.ts",
    "index.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "format": "prettier **/**/*.ts --ignore-path ./.prettierignore --write",
    "precommit": "lint-staged",
    "prepublish:npm": "npm run build",
    "publish:npm": "npm publish --access public",
    "prepublish:next": "npm run build",
    "publish:next": "npm publish --access public --tag next",
    "test:e2e": "jest --config ./tests/jest-e2e.json",
    "pretest:examples": "npm run build",
    "test:examples": "cd examples && tsc"
  },
  "author": "Nick Bolles <me@nickbolles.com>",
  "license": "ISC",
  "devDependencies": {
    "@accounts/password": "^0.19.0",
    "@nestjs/common": "^6.6.4",
    "@nestjs/core": "^6.6.4",
    "@nestjs/graphql": "^6.5.0",
    "@nestjs/platform-express": "^6.6.4",
    "@nestjs/testing": "^6.6.4",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.3",
    "@types/supertest": "^2.0.8",
    "apollo-server-express": "^2.7.0",
    "husky": "3.0.5",
    "jest": "24.9.0",
    "lint-staged": "9.2.5",
    "nest-router": "^1.0.9",
    "nestjs-config": "^1.4.4",
    "prettier": "1.18.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "supertest": "4.0.2",
    "ts-jest": "24.0.2",
    "ts-node": "8.3.0",
    "type-graphql": "^0.17.4"
  },
  "peerDependencies": {
    "@accounts/password": "~0.15.0",
    "@graphql-modules/core": "^0.7.10",
    "@nestjs/core": "~6.2.0",
    "reflect-metadata": "~0.1.12",
    "typescript": ">3.5.0"
  },
  "lint-staged": {
    "*.ts": [
      "prettier --ignore-path ./.prettierignore --write",
      "git add"
    ]
  },
  "dependencies": {
    "@accounts/graphql-api": "0.*",
    "@accounts/rest-express": "0.*",
    "@accounts/server": "0.*",
    "@accounts/types": "0.*"
  },
  "directories": {
    "example": "examples",
    "lib": "lib",
    "test": "test"
  }
}
